#!/usr/bin/make -f

%:
	dh $@

override_dh_auto_configure:
	dh_auto_configure -- \
		-DCMAKE_INSTALL_PREFIX="/usr/local/"
		-DINSTALL_INCLUDE=OFF

override_dh_auto_build:
	dh_auto_build

override_dh_auto_install:
	dh_auto_install

override_dh_usrlocal:

override_dh_shlibdeps:

override_dh_auto_install:
	dh_auto_install

